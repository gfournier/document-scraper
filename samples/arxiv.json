{
	"rootUrl": "https://arxiv.org/search/cs",
	"baseUrl": "https://arxiv.org",
	"form": {
		"method": "GET",
		"fields": [
			{
				"name": "query",
				"value": "deep learning"
			},
			{
				"name": "searchtype",
				"value": "all"
			},
			{
				"name": "abstracts",
				"value": "hide"
			},
			{
				"name": "order",
				"value": "-announced_date_first"
			},
			{
				"name": "size",
				"value": "25"
			}
		]
	},
	"nextPage": {
      "xpath": "xpath_to_next_page_link"
    },
	"item": {
		"output": {
			"metadata": {
				"date": {
					"xpath": "//div[@class=\"dateline\"]",
					"regex": "Submitted on (.*)]"
				}
			}
		}
	}
}
